<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    app:startDestination="@id/dashBoardRentishFragment"
    android:id="@+id/rents">
    <fragment
        android:id="@+id/dashBoardRentishFragment"
        android:name="com.example.rantish.Rents.fragment.DashBoardRentishFragment"
        android:label="fragment_dash_board_rentish"
        tools:layout="@layout/fragment_dash_board_rentish" >
        <action
            android:id="@+id/action_dashBoardRentishFragment_to_createPostFragment"
            app:destination="@id/createPostFragment" />
        <action
            android:id="@+id/action_dashBoardRentishFragment_to_postViewFragment"
            app:destination="@id/postViewFragment" />
        <action
            android:id="@+id/action_dashBoardRentishFragment_to_requestFragment"
            app:destination="@id/requestFragment" />
    </fragment>
    <fragment
        android:id="@+id/createPostFragment"
        android:name="com.example.rantish.Rents.fragment.CreatePostFragment"
        android:label="fragment_create_post"
        tools:layout="@layout/fragment_create_post" />
    <fragment
        android:id="@+id/postViewFragment"
        android:name="com.example.rantish.Rents.fragment.PostViewFragment"
        android:label="fragment_post_view"
        tools:layout="@layout/fragment_post_view" />
    <fragment
        android:id="@+id/requestFragment"
        android:name="com.example.rantish.Rents.fragment.RequestFragment"
        android:label="fragment_request"
        tools:layout="@layout/fragment_request" >
        <action
            android:id="@+id/action_requestFragment_to_freeLancerFragment"
            app:destination="@id/freeLancerFragment" />
    </fragment>
    <fragment
        android:id="@+id/freeLancerFragment"
        android:name="com.example.rantish.Rents.fragment.FreeLancerFragment"
        android:label="fragment_free_lancer"
        tools:layout="@layout/fragment_free_lancer" />
    <fragment
        android:id="@+id/requestSentFragment"
        android:name="com.example.rantish.Rents.fragment.RequestSentFragment"
        android:label="fragment_request_sent"
        tools:layout="@layout/fragment_request_sent" />

</navigation>